{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:frontend\" --names \"server,frontend\" --prefix-colors \"blue,green\"",
    "dev:server": "tsx watch --clear-screen=false server/index.ts",
    "dev:frontend": "vite --port 3000",
    "dev:memory": "cross-env NODE_ENV=development PORT=3005 tsx watch --clear-screen=false server/index.ts",
    "dev:build": "npm run build && npm run dev:memory",
    "dev:safe": "node scripts/start-dev.js",
    "dev:clean": "npx kill-port 5000 && npx kill-port 3000 && npm run dev",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "cross-env NODE_ENV=production PORT=3005 node dist/index.js",
    "start:safe": "npx kill-port 3005 && npm run start",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "db:init": "tsx server/init-db.ts",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "port:clean": "npx kill-port 5000",
    "process:clean": "pkill -f \"tsx watch\" || true",
    "postinstall": "npm install --os=linux --cpu=x64 sharp"
  },
